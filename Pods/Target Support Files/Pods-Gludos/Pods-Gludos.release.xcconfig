EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/DLRadioButton.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Spring.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwiftOverlays.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwipeView.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "DLRadioButton" -framework "Spring" -framework "SwiftOverlays" -framework "SwipeView"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Gludos
PODS_ROOT = ${SRCROOT}/Pods